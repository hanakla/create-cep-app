{
  "private": true,
  "name": "{{appName}}",
  "scripts": {
    "start": "run-p dev:*",
    "dev:symlink": "node -r 'esbuild-register' ./build/symlink.ts",
    "dev:host": "parcel watch --no-hmr --public-url ./ -d ./dist/host host/index.ts",
    "dev:client": "node -r 'esbuild-register' ./node_modules/.bin/webpack --watch"
  },
  "dependencies": {
    "domready": "^1.0.8",
    "es6-string-polyfills": "*",
    "extendscript-es5-shim-ts": "*",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3",
    "styled-reset": "^4.3.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/domready": "^1.0.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/styled-components": "^5.1.24",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^2.0.6",
    "esbuild": "^0.14.25",
    "esbuild-register": "^3.3.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "1.12.3",
    "types-for-adobe": "^7.0.0",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  }
}
