{
  "name": "@hanakla/create-react-ts-cep-app",
  "version": "0.0.1-beta.6",
  "description": "Create Adobe CEF extension with React/TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Hanakla<hanakla.dev@gmail.com>",
  "repository": "https://github.com/hanakla/create-react-ts-cep-app",
  "license": "MIT",
  "bin": {
    "create-react-ts-cep-app": "./bin/index.js"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:cli": "ncc build src/cli.ts -o ./bin -e yargs --minify --no-cache --no-source-map-register",
    "build:lib": "tsc",
    "dev": "run-p watch:*",
    "watch:cli": "ncc build --watch src/cli.ts -o ./bin",
    "watch:lib": "tsc --watch",
    "example": "rm -rf ./test-build; ts-node -T src/index.ts test-build",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "chalk": "^5.0.0",
    "commander": "^9.0.0",
    "cpy": "^9.0.0",
    "dedent": "^0.7.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "prompts": "^2.4.2",
    "validate-npm-package-name": "^3.0.0",
    "xml-formatter": "^2.6.1",
    "yargs": "^17.3.1"
  },
  "peerDependencies": {
    "ts-node": "^10.7.0"
  },
  "devDependencies": {
    "@types/dedent": "^0.7.0",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^17.0.21",
    "@types/prompts": "^2.0.14",
    "@types/validate-npm-package-name": "^3.0.3",
    "@types/yargs": "^17.0.9",
    "@vercel/ncc": "^0.33.3",
    "typescript": "^4.6.2"
  }
}
