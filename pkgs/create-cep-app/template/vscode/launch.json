    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Photoshop",
      "port": 8081,
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Illustrator",
      "port": 8082,
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "chrome",
      "request": "attach",
