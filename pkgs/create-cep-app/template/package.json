{
  "private": true,
  "name": "app-name",
  "version": "0.0.0",
  "scripts": {
    "start": "run-p -l dev:*",
    "build": "run-p -l build:* && cep-app-scripts pack",
    "build:manifest": "cep-app-scripts manifest",
    "build:host": "parcel build --public-url ./ -d ./dist/host host/*.ts",
    "build:client": "ts-node -T ./node_modules/.bin/webpack",
    "dev:manifest": "cep-app-scripts manifest",
    "dev:symlink": "cep-app-scripts symlink",
    "dev:host": "parcel watch --no-hmr --public-url ./ -d ./dist/host src/host/*.ts",
    "dev:client": "ts-node -T ./node_modules/.bin/webpack --watch"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.4.0",
    "@hanakla/cep-utils": "*",
    "@spectrum-web-components/bundle": "^0.24.10",
    "@spectrum-web-components/icons": "^0.8.4",
    "@spectrum-web-components/icons-workflow": "^0.8.5",
    "csinterface": "git+https://github.com/ExtendScript/CSInterface.git",
    "domready": "^1.0.8",
    "es6-string-polyfills": "*",
    "extendscript-es5-shim": "^0.3.1",
    "extendscript-es6-shim": "^0.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "styled-components": "^5.3.3",
    "styled-reset": "^4.3.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@hanakla/cep-app-scripts": "*",
    "@types/domready": "^1.0.0",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/styled-components": "^5.1.24",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^2.0.6",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "1.12.3",
    "ts-node": "^10.7.0",
    "types-for-adobe": "^7.0.0",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  }
}
